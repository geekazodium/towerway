[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://rust-lib/target/debug/librust_lib.so"
linux.release.x86_64 =   "res://rust-lib/target/release/librust_lib.so"
windows.debug.x86_64 =   "res://rust-lib/target/debug/rust_lib.dll"
windows.release.x86_64 = "res://rust-lib/target/release/rust_lib.dll"
macos.debug =            "res://rust-lib/target/debug/librust_lib.dylib"
macos.release =          "res://rust-lib/target/release/librust_lib.dylib"
macos.debug.arm64 =      "res://rust-lib/target/debug/librust_lib.dylib"
macos.release.arm64 =    "res://rust-lib/target/release/librust_lib.dylib"